{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/docs/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appName": "JulianSangillo.Reflection",
      "_appTitle": "Documentation",
      "_appFooter": "<span>Julian Sangillo<br>Generated by <strong>DocFX</strong></span>",
      "_appLogoPath": "assets/logo.svg",
      "_appFaviconPath": "assets/favicon.ico",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true,
      "_disableViewSource": false
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/discordfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}